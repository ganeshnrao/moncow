{
  "name": "moncow",
  "displayName": "MonCow",
  "description": "Run MongoDB queries directly from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "publisher": "ganeshnrao",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:moncow.runFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "moncow.runFile",
        "key": "Cmd+Shift+1"
      }
    ],
    "commands": [
      {
        "command": "moncow.runFile",
        "title": "MonCow: Run file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.146",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.10",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.7.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "log-row": "^2.0.3",
    "mongodb": "^3.3.3"
  }
}
