{
  "name": "moncow",
  "displayName": "MonCow",
  "description": "Run MongoDB queries directly from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:moncow.runFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "moncow.runFile",
        "key": "Cmd+Shift+1"
      }
    ],
    "commands": [
      {
        "command": "moncow.runFile",
        "title": "MonCow: Run file"
      }
    ],
    "configuration": {
      "title": "MonCow",
      "properties": {
        "moncow.apiUrl": {
          "type": "string",
          "default": "http://localhost:2020/api",
          "description": "MonCow API server URL",
          "scope": "window",
          "examples": [
            "http://localhost:2020/api"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
